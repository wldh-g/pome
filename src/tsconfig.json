{
  "compilerOptions": {
    "jsx": "preserve",
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "esnext",
      "dom"
    ],
    "allowJs": true,
    "sourceMap": true,
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "keyofStringsOnly": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "./components/**/*",
    "./pages/**/*",
    "./scss.d.ts",
    "./kakao.d.ts"
  ]
}
